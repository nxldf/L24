<script>
    $("html, body").animate({ scrollTop: 0 }, "slow");

    function callbackpage() {
        $.ajax({
            type: 'get',
            url: '/@CultureHelper.GetCurrentCulture()/upload/Setep7',
            cache: false,
            contentType: false,
            processData: false,
            success: function (data) {
                $('#updatediv').html(data);
            },
            error: function (data) {

            }
        });
    }

    function change_framedBtn(val) {
        var inp = $('#frame');
        var btnyes = $('#frame_yes');
        var btnno = $('#frame_no');
        if (val == 1) {
            inp.val('true');
            btnyes.addClass('button--active');
            btnno.removeClass('button--active');
            $('#framedetail').show();
        } else {
            inp.val('false');
            btnno.addClass('button--active');
            btnyes.removeClass('button--active');
            $('#framedetail').hide();
        }
    }
    function change_multipanel(val) {
        var inp = $('#multipanel');
        var btnyes = $('#multipanel_yes');
        var btnno = $('#multipanel_no');
        if (val == 1) {
            inp.val('true');
            btnyes.addClass('button--active');
            btnno.removeClass('button--active');
        } else {
            inp.val('false');
            btnno.addClass('button--active');
            btnyes.removeClass('button--active');   
        }
    }
    function change_pack(val) {
        var inp = $('#packing');
        var packing_crate = $('#packing_crate');
        var packing_box = $('#packing_box');
        var packing_tube = $('#packing_tube');

        packing_crate.removeClass('image-radio__item--active');
        packing_box.removeClass('image-radio__item--active');
        packing_tube.removeClass('image-radio__item--active');
        if (val == 1) {
            inp.val('0');
            packing_crate.addClass('image-radio__item--active');
            $('#frameoption').show();
            
        } else if (val == 2) {
            inp.val('1');
            packing_box.addClass('image-radio__item--active');
            $('#frameoption').show();
            
        } else {
            inp.val('2');
            packing_tube.addClass('image-radio__item--active');
            $('#frameoption').hide();
            $('#framedetail').hide();
            
        }
    }
</script>